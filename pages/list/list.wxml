<!--pages/list/list.wxml-->
<g-box mode="border" title="基础布局">
  <g-list>我左对齐</g-list>
  <g-list align="tc">我居中对齐</g-list>
  <g-list align="tr">我右对齐</g-list>
  <g-list title="我是标题">我是内容</g-list>
  <g-list title="多行显示">
    <view>我是内容1</view>
    <view>我是内容2</view>
  </g-list>
  <g-list mode="border" title="居中显示" vertical>
    <view>我是内容1</view>
    <view>我是内容2</view>
  </g-list>
</g-box>

<g-box mode="border" title="带箭头">
  <g-list link>我是标题也是内容</g-list>
  <g-list title="我是标题" link>我是内容</g-list>
  <g-list title="我是标题" ellipsis link>我是超长的内容，超出会自动隐藏显示省略号</g-list>
</g-box>

<g-box mode="border" title="带图标">
  <g-list icon="data:image/png;base64," link>
    <view>我是内容</view>
  </g-list>
</g-box>

<g-box mode="border" title="头像">
  <g-list avatar="https://p3.pstatp.com/thumb/7be8000268c22a5e9d23.jpeg" link>Gensp</g-list>
</g-box>

<g-box mode="border" title="圆角" mode="empty">
  <g-list mode="radius">我是内容</g-list>
  <g-list title="我是标题" mode="radius">我是内容</g-list>
  <g-list avatar="https://p3.pstatp.com/thumb/7be8000268c22a5e9d23.jpeg" mode="radius">Gensp</g-list>
</g-box>